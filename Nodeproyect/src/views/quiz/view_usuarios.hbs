<div class="container">
    <div class="row">
        <h1>Lista de Estudiantes</h1>
    </div>
        <div class="col-12">
            <table class="table">
                <thead>
                    <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Nombre</th>
                    <th scope="col">Cedula</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each result_usuarios}}
                    <tr>
                    <th scope="row">{{id_usuarios}}</th>
                    <td>{{nombre}}</td>
                    <td>{{cedula}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
</div>